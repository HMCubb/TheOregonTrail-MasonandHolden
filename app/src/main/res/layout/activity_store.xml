        android:id="@+id/clothesText"
        android:layout_width="244dp"
        android:layout_height="0dp"
        android:layout_marginStart="175dp"
        android:layout_marginEnd="173dp"
        android:background="#FFFFFF"
        android:text="Clothes ($10/set)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/clothesDown"
        app:layout_constraintEnd_toEndOf="@+id/clothesAmount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/axlesText" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/foodUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="430dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/foodDown"
        app:layout_constraintStart_toStartOf="@+id/headerText2"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <EditText
        android:id="@+id/tonguesAmount"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FFFFFF"
