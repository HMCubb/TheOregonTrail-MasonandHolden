        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toEndOf="@+id/clothesAmount"
        app:layout_constraintTop_toBottomOf="@+id/tonguesText" />

    <TextView
        android:id="@+id/tonguesText"
        android:layout_width="238dp"
