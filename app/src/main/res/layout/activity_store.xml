        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/foodText" />

    <Button
        android:id="@+id/foodUp"
        android:layout_width="49dp"
        android:layout_height="56dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="73dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/foodText"
        app:layout_constraintTop_toBottomOf="@+id/headerText2"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <TextView
        android:id="@+id/tonguesAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="111dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toEndOf="@+id/tonguesDown"
        app:layout_constraintTop_toBottomOf="@+id/headerText2" />

    <TextView
        android:id="@+id/clothesAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="114dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toEndOf="@+id/clothesUp"
        app:layout_constraintTop_toBottomOf="@+id/foodAmount" />

    <TextView
        android:id="@+id/wheelsAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="118dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toEndOf="@+id/wheelsDown"
        app:layout_constraintTop_toBottomOf="@+id/headerText2" />

    <TextView
        android:id="@+id/moneyOwedAmount"
        android:layout_width="217dp"
        android:layout_height="66dp"
        android:layout_marginStart="31dp"
        android:layout_marginBottom="87dp"
        android:background="#FFFFFF"
        android:text="$0.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/moneyOwedText" />

    <TextView
        android:id="@+id/moneyOwedText"
        android:layout_width="197dp"
        android:layout_height="65dp"
        android:layout_marginStart="112dp"
        android:layout_marginBottom="88dp"
        android:background="#FFFFFF"
        android:text="Owed $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/yourMoneyAmount"
        android:layout_width="217dp"
        android:layout_height="66dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="188dp"
        android:background="#FFFFFF"
        android:text="$850.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintStart_toEndOf="@+id/yourMoneyText"
        app:layout_constraintTop_toBottomOf="@+id/clothesText" />

    <TextView
        android:id="@+id/yourMoneyText"
        android:layout_width="197dp"
        android:layout_height="65dp"
        android:layout_marginStart="112dp"
        android:layout_marginTop="188dp"
        android:background="#FFFFFF"
        android:text="Your $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clothesText" />

    <TextView
        android:id="@+id/headerText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:background="#FAF7F7"
        android:text="What would you like to take with your party?"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#0B0A0A"
        android:textSize="82sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/continueButton"
        android:layout_width="339dp"
        android:layout_height="117dp"
        android:layout_marginStart="234dp"
        android:layout_marginBottom="87dp"
        android:background="#F8F4F4"
        android:backgroundTint="@color/white"
        android:backgroundTintMode="add"
        android:foregroundTint="#F8F5F5"
        android:text="continue"
        android:textColor="#050505"
        android:textColorHighlight="#F8F5F5"
        android:textColorLink="#F8EDED"
        android:textSize="48sp"
        android:typeface="serif"
        app:iconTint="#EFE8EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/moneyOwedAmount" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>