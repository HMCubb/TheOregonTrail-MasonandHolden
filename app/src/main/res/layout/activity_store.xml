        android:layout_marginBottom="205dp"
        android:background="#FFFFFF"
        android:text="axles  ($10)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/yourMoneyAmount"
        app:layout_constraintEnd_toEndOf="@+id/axlesAmount"
        app:layout_constraintStart_toStartOf="@+id/clothesAmount"
        app:layout_constraintTop_toTopOf="@+id/wheelsText" />

    <TextView
        android:id="@+id/tonguesText"
