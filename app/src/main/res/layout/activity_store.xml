        app:layout_constraintBottom_toBottomOf="@+id/clothesDown"
        app:layout_constraintEnd_toStartOf="@+id/oxenText"
        app:layout_constraintStart_toStartOf="@+id/axlesUp"
        app:layout_constraintTop_toTopOf="@+id/axlesUp" />

    <EditText
        android:id="@+id/foodAmount"
        android:layout_width="0dp"
        android:layout_height="67dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/foodText"
        app:layout_constraintEnd_toEndOf="@+id/clothesAmount"
