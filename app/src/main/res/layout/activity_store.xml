        android:layout_marginEnd="138dp"
        android:layout_marginBottom="90dp"
        android:background="#FFFFFF"
        android:text="10lbs food ($2/10lbs)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/clothesAmount"
        app:layout_constraintEnd_toEndOf="@+id/yourMoneyAmount"
        app:layout_constraintStart_toStartOf="@+id/headerText2"
        app:layout_constraintTop_toBottomOf="@+id/headerText2" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/foodDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/foodAmount"
        app:layout_constraintStart_toStartOf="@+id/foodUp"
        app:layout_constraintTop_toBottomOf="@+id/foodAmount"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/clothesUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/axlesUp"
        app:layout_constraintStart_toStartOf="@+id/foodDown"
        app:layout_constraintTop_toTopOf="@+id/axlesUp"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
