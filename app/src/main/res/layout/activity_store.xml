        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/foodAmount"
        app:layout_constraintEnd_toEndOf="@+id/axlesAmount"
        app:layout_constraintStart_toStartOf="@+id/axlesAmount"
        app:layout_constraintTop_toTopOf="@+id/foodAmount" />

    <EditText
        android:id="@+id/clothesAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="83dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="33dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/clothesDown"
        app:layout_constraintEnd_toStartOf="@+id/tonguesText"
        app:layout_constraintStart_toStartOf="@+id/clothesUp"
        app:layout_constraintTop_toTopOf="@+id/clothesUp" />

    <EditText
        android:id="@+id/wheelsAmount"
        android:layout_width="0dp"
        android:layout_height="67dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/wheelsDown"
        app:layout_constraintEnd_toEndOf="@+id/oxenAmount"
        app:layout_constraintStart_toStartOf="@+id/oxenAmount"
        app:layout_constraintTop_toTopOf="@+id/wheelsDown" />

    <EditText
        android:id="@+id/moneyOwedAmount"
        android:layout_width="0dp"
        android:layout_height="66dp"
        android:layout_marginTop="93dp"
        android:layout_marginBottom="87dp"
        android:background="#FFFFFF"
        android:text="$0.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/yourMoneyAmount"
        app:layout_constraintStart_toStartOf="@+id/yourMoneyAmount"
        app:layout_constraintTop_toTopOf="@+id/yourMoneyAmount" />

    <TextView
        android:id="@+id/moneyOwedText"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:background="#FFFFFF"
        android:text="Owed $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintBaseline_toBaselineOf="@+id/moneyOwedAmount"
        app:layout_constraintEnd_toEndOf="@+id/yourMoneyText"
        app:layout_constraintStart_toStartOf="@+id/yourMoneyText" />

    <EditText
        android:id="@+id/yourMoneyAmount"
        android:layout_width="0dp"
        android:layout_height="66dp"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="180dp"
        android:background="#FFFFFF"
        android:text="$850.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/clothesAmount"
        app:layout_constraintStart_toEndOf="@+id/yourMoneyText" />

    <TextView
        android:id="@+id/yourMoneyText"
        android:layout_width="197dp"
        android:layout_height="65dp"
        android:layout_marginStart="112dp"
        android:layout_marginEnd="110dp"
        android:background="#FFFFFF"
        android:text="Your $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintBaseline_toBaselineOf="@+id/yourMoneyAmount"
        app:layout_constraintEnd_toEndOf="@+id/clothesText"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/headerText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#FAF7F7"
        android:text="What would you like to take with your party?"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#0B0A0A"
        android:textSize="82sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/continueButton"
        android:layout_width="339dp"
        android:layout_height="117dp"
        android:layout_marginBottom="87dp"
        android:background="#F8F4F4"
        android:backgroundTint="@color/white"
        android:backgroundTintMode="add"
        android:foregroundTint="#F8F5F5"
        android:text="continue"
        android:textColor="#050505"
        android:textColorHighlight="#F8F5F5"
        android:textColorLink="#F8EDED"
        android:textSize="48sp"
        android:typeface="serif"
        app:iconTint="#EFE8EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>