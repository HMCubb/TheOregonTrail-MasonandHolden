<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Store"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".Store">

    <TextView
        android:id="@+id/moneyOwedAmount"
        android:layout_width="217dp"
        android:layout_height="66dp"
        android:layout_marginStart="228dp"
        android:layout_marginEnd="221dp"
        android:background="#FFFFFF"
        android:text="$850.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/moneyOwedText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/moneyOwedText" />

    <TextView
        android:id="@+id/yourMoneyText"
        android:layout_width="197dp"
        android:layout_height="65dp"
        android:layout_marginStart="147dp"
        android:layout_marginBottom="29dp"
        android:background="#FFFFFF"
        android:text="Your $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/moneyOwedText"
        app:layout_constraintStart_toEndOf="@+id/axlesAmount" />

    <TextView
        android:id="@+id/moneyOwedText"
        android:layout_width="0dp"
        android:layout_height="65dp"
        android:layout_marginBottom="65dp"
        android:background="#FFFFFF"
        android:text="Owed $"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/continueButton"
        app:layout_constraintEnd_toEndOf="@+id/yourMoneyText"
        app:layout_constraintStart_toStartOf="@+id/yourMoneyText" />

    <TextView
        android:id="@+id/axlesAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="33dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/axlesDown"
        app:layout_constraintStart_toEndOf="@+id/axlesDown"
        app:layout_constraintTop_toTopOf="@+id/axlesUp" />

    <TextView
        android:id="@+id/foodAmount"
        android:layout_width="0dp"
        android:layout_height="67dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/foodText"
        app:layout_constraintEnd_toEndOf="@+id/clothesAmount"
        app:layout_constraintStart_toStartOf="@+id/clothesAmount"
        app:layout_constraintTop_toTopOf="@+id/foodText" />

    <TextView
        android:id="@+id/wheelsText"
        android:layout_width="238dp"
        android:layout_height="0dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="180dp"
        android:layout_marginBottom="7dp"
        android:background="#FFFFFF"
        android:text="wheells ($0.20/lb)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/tonguesDown"
        app:layout_constraintEnd_toEndOf="@+id/wheelsAmount"
        app:layout_constraintStart_toStartOf="@+id/tonguesAmount"
        app:layout_constraintTop_toTopOf="@+id/wheelsUp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/wheelsDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/wheelsText"
        app:layout_constraintStart_toEndOf="@+id/wheelsText"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/wheelsUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="72dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/wheelsText"
        app:layout_constraintTop_toBottomOf="@+id/headerText2"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <TextView
        android:id="@+id/axlesText"
        android:layout_width="244dp"
        android:layout_height="0dp"
        android:layout_marginStart="174dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="176dp"
        android:background="#FFFFFF"
        android:text="axles  ($10/set)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/axlesDown"
        app:layout_constraintEnd_toEndOf="@+id/axlesAmount"
        app:layout_constraintStart_toStartOf="@+id/clothesAmount"
        app:layout_constraintTop_toTopOf="@+id/axlesUp" />

    <TextView
        android:id="@+id/tonguesText"
        android:layout_width="238dp"
        android:layout_height="0dp"
        android:layout_marginStart="518dp"
        android:layout_marginEnd="519dp"
        android:background="#FFFFFF"
        android:text="tongues ($0.20/lb)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/tonguesDown"
        app:layout_constraintEnd_toEndOf="@+id/wheelsText"
        app:layout_constraintStart_toStartOf="@+id/clothesText"
        app:layout_constraintTop_toTopOf="@+id/foodText" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/axlesUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/axlesDown"
        app:layout_constraintStart_toEndOf="@+id/axlesText"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/tonguesDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tonguesAmount"
        app:layout_constraintStart_toEndOf="@+id/tonguesText"
        app:layout_constraintTop_toBottomOf="@+id/tonguesAmount"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/axlesDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="23dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/yourMoneyText"
        app:layout_constraintEnd_toEndOf="@+id/continueButton"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/toungesUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/tonguesDown"
        app:layout_constraintStart_toEndOf="@+id/tonguesText"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <TextView
        android:id="@+id/foodText"
        android:layout_width="284dp"
        android:layout_height="131dp"
        android:layout_marginStart="135dp"
        android:layout_marginTop="86dp"
        android:layout_marginBottom="90dp"
        android:background="#FFFFFF"
        android:text="10lbs food ($2/10lbs)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/clothesAmount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headerText2" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/foodDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/foodAmount"
        app:layout_constraintStart_toEndOf="@+id/foodText"
        app:layout_constraintTop_toBottomOf="@+id/foodAmount"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/clothesUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/axlesUp"
        app:layout_constraintStart_toEndOf="@+id/foodText"
        app:layout_constraintTop_toTopOf="@+id/axlesUp"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/clothesDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginTop="21dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/clothesText"
        app:layout_constraintTop_toBottomOf="@+id/clothesUp"
        app:srcCompat="@android:drawable/arrow_down_float" />

    <TextView
        android:id="@+id/clothesText"
        android:layout_width="244dp"
        android:layout_height="0dp"
        android:layout_marginStart="175dp"
        android:layout_marginEnd="173dp"
        android:background="#FFFFFF"
        android:text="Clothes ($10/set)"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/clothesDown"
        app:layout_constraintEnd_toEndOf="@+id/clothesAmount"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/axlesText" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/foodUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="11dp"
        android:layout_marginBottom="21dp"
        android:clickable="true"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/foodDown"
        app:layout_constraintStart_toEndOf="@+id/foodText"
        app:srcCompat="@android:drawable/arrow_up_float" />

    <TextView
        android:id="@+id/tonguesAmount"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/foodAmount"
        app:layout_constraintEnd_toEndOf="@+id/axlesAmount"
        app:layout_constraintStart_toStartOf="@+id/axlesAmount"
        app:layout_constraintTop_toTopOf="@+id/foodAmount" />

    <TextView
        android:id="@+id/clothesAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="33dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="@+id/clothesDown"
        app:layout_constraintStart_toEndOf="@+id/clothesUp"
        app:layout_constraintTop_toTopOf="@+id/clothesUp" />

    <TextView
        android:id="@+id/wheelsAmount"
        android:layout_width="79dp"
        android:layout_height="67dp"
        android:background="#FFFFFF"
        android:text="0"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/wheelsDown"
        app:layout_constraintEnd_toEndOf="@+id/yourMoneyAmount"
        app:layout_constraintStart_toStartOf="@+id/yourMoneyAmount"
        app:layout_constraintTop_toTopOf="@+id/wheelsDown" />

    <TextView
        android:id="@+id/yourMoneyAmount"
        android:layout_width="0dp"
        android:layout_height="66dp"
        android:background="#FFFFFF"
        android:text="$850.00"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="48sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/yourMoneyText"
        app:layout_constraintEnd_toEndOf="@+id/moneyOwedAmount"
        app:layout_constraintStart_toStartOf="@+id/moneyOwedAmount" />

    <TextView
        android:id="@+id/headerText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="#FAF7F7"
        android:text="What would you like to take with your party?"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="#0B0A0A"
        android:textSize="82sp"
        android:textStyle="bold"
        android:typeface="serif"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/continueButton"
        android:layout_width="339dp"
        android:layout_height="117dp"
        android:layout_marginBottom="87dp"
        android:background="#F8F4F4"
        android:backgroundTint="@color/white"
        android:backgroundTintMode="add"
        android:foregroundTint="#F8F5F5"
        android:text="continue"
        android:textColor="#050505"
        android:textColorHighlight="#F8F5F5"
        android:textColorLink="#F8EDED"
        android:textSize="48sp"
        android:typeface="serif"
        app:iconTint="#EFE8EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>